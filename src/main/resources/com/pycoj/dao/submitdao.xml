<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pycoj.dao.SubmitDao">
    <insert id="save">
        INSERT INTO `submit`
        (`user_id`,`question_id`,`state`,`m_cost`,`t_cost`,`dir`)
        VALUES(#{submit.userId},#{submit.questionId},#{submit.state.state},#{submit.mCost},#{submit.tCost},#{submit.dir})
    </insert>
</mapper>